{
  "name": "@geoblocks/edittrack",
  "version": "1.1.3",
  "description": "Geoblocks edittrack",
  "module": "src/index.js",
  "scripts": {
    "eslint": "eslint src test",
    "eslint-fix": "eslint src test --fix",
    "start": "parcel serve demos/*.html",
    "build-demo": "parcel build --target demos demos/*.html",
    "test": "mocha --exit",
    "typecheck": "tsc --pretty",
    "lint": "npm run eslint && npm run typecheck",
    "doc": "typedoc",
    "gh-pages": "rm -rf docs && npm run doc && npm run build-demo && gh-pages -d dist"
  },
  "license": "BSD-3-Clause",
  "repository": "github:geoblocks/edittrack",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src"
  ],
  "type": "module",
  "targets": {
    "demos": {
      "context": "browser",
      "publicUrl": "./"
    }
  },
  "devDependencies": {
    "@geoblocks/d3profile": "0.0.6",
    "@geoblocks/proj": "0.4.6",
    "@geoblocks/sources": "0.3.1",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/proj4": "2.5.2",
    "chai": "4.3.7",
    "eslint": "8.36.0",
    "eslint-config-openlayers": "18.0.0",
    "gh-pages": "5.0.0",
    "mocha": "10.2.0",
    "ol": "7.3.0",
    "parcel": "2.8.3",
    "proj4": "2.9.0",
    "tsconfig-paths": "4.1.2",
    "typedoc": "0.23.28",
    "typescript": "5.0.3"
  },
  "optionalDependencies": {
    "@geoblocks/d3profile": "0.0.6"
  },
  "peerDependencies": {
    "ol": "^6 || ^7",
    "proj4": "^2.4.4"
  }
}
