{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [{
    "matchUpdateTypes": ["minor", "patch"],
    "groupName": "all patch and minor dependencies",
    "automerge": true
  }]
}
